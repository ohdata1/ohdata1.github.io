{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Pollution emissions in Germany",
    "subtitle": "Daily average carbon monoxide levels in Germany",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/ohdata1/ohdata1.github.io/main/data_Emissions-DE.json",
    "format": {"type": "json"}
  },
  "height": 300,
  "width": 310,
  "mark": {"type": "point", "color": "#EEC522"},
  "encoding": {
    "x": {
      "field": "start",
      "type": "temporal",
      "title": null,
      "axis": {"grid": false}
    },
    "y": {
      "field": "average",
      "type": "quantitative",
      "title": null,
      "axis": {"grid": false}
    },
    "color": {"type": "nominal", "scale": {"scheme": "dark2"}}
  },
  "config": {}
}
