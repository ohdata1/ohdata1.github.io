{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Denmark per capita income over time",
    "subtitle": "GDP per capita (USD) constant 2010 prices. Source: Eurostat",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/ohdata1/ohdata1.github.io/main/dataproject/Denmark%20gdp%20per%20capita.csv",
    "format": {"type": "csv"}
  },
  "height": 300,
  "width": 310,
  "mark": {"type": "line", "point": false, "color": "#6600CC"},
  "encoding": {
    "x": {
      "field": "time",
      "type": "temporal",
      "title": null,
      "axis": {"grid": false}
    },
    "y": {
      "field": "GDP per capita",
      "type": "quantitative",
      "title": null,
      "axis": {"grid": false}
    }
  },
  "config": {}
}
